@model IEnumerable<RooyaDev.Models.Order>

@{
    ViewBag.Title = "Orders";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div id="right-panel" class="right-panel">

    <!-- Header-->
    <header id="header" class="header">
        <div class="header-menu">
            <div class="col-sm-7">
                <a id="menuToggle" class="menutoggle pull-left"><i class="fa fa fa-tasks"></i></a>
                <div class="center">
                    <i class="fa fa-cart-plus"> </i> <label> Orders</label>
                </div>

            </div>
        </div>

    </header><!-- /header -->
    <!-- Header-->


    <div class="col-md-12">
        <div class="panel panel-default">
            <!-- Default panel contents -->
            <div class="panel-heading"></div>


            <!-- Table -->
            <table class="table table-hover table-bordered">
                <tr>
                    <td>Date</td>
                    <td>User</td>
                    <td>Price</td>
                    
                </tr>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>@item.Date</td>
                        <td class="col-md-4">
                            <a href="~/Users/Details/@item.User_ID" class="btn btn-info">User</a>
                        </td>
                        <td class="col-md-4">
                            <a href="~/Pricings/Details/@item.User_ID" class="btn btn-primary">
                                Price
                            </a>
                        </td>

                    </tr>

                }               

            </table>
        </div>
    </div>
